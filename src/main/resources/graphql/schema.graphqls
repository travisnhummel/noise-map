type NoiseRecord {
  id: ID!
  suburb: String!
  latitude: Float!
  longitude: Float!
  noiseLevel: Float!
  timestamp: String!
}

type Query {
  allNoise: [NoiseRecord]
  noiseBySuburb(suburb: String!): [NoiseRecord]
}

type Mutation {
  addNoiseRecord(
    suburb: String!
    decibel: Float!
    latitude: Float!
    longitude: Float!
    timestamp: String!
  ): NoiseRecord
}
